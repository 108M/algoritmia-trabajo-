#include<ctype.h>
#include <stdlib.h>
#include <stdio.h>
#include "colaEnteros.h"
int main(){
	tipoCola c;
    char nombre [50], basura;
    int a,i,aux,elem;
    int t[8];
    FILE *f;
    i = -1;
    nuevaCola(&c);
    printf( "Introduzca el nombre del fichero a leer: \n");
    scanf (" %s", nombre);
    f=fopen(nombre,"r");
  
    while (!feof(f))
    {
        if (fscanf (f,"%d",&a) == 1 && basura == ','){
            aux = a;
           printf("%d ", a);
           if ( i > 8){
               encolar(&c,t);
               printf("reset de  i\n");
               
               i = 0;
        }
           i = i + 1;
           t[i] = aux;
           printf("%d ", t[i]);
          
        }else{
            
            basura=fgetc(f);
        }
        
    }
    
    fclose;
    desencolar(&c);
    elem = frente(c);
    printf("%d", elem);

    return 0;
}
